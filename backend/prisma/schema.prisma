generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Technology {
  id          String  @id @default(uuid())
  name        String
  description String
  image       String
  highlighted Boolean @default(false)

  @@map("technology")
}
